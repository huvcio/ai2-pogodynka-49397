{
    "city": "{{ city }}",
    "country": "{{ country }}",
    "measurements": [
        {% for measurement in measurements %}
        <br>
        {
            "date": "{{ measurement.Date | date('Y-m-d') }}",
            "celsius": "{{ measurement.celsius }}",
            "fahrenheit": "{{ measurement.fahrenheit }}"
        }
        {% if not loop.last %},{% endif %}
        {% endfor %}
    ]
}